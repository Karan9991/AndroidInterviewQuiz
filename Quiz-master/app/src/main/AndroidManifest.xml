<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.quizexampleinterview">

    <application
        android:allowBackup="true"
        android:icon="@drawable/iccon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.test.quizexampleinterview.Forgot_Password"></activity>
        <activity android:name="com.test.quizexampleinterview.SignUp" />
        <activity android:name="com.test.quizexampleinterview.SignIn" />
        <activity
            android:name="com.test.quizexampleinterview.QuizMainActivity"
            android:configChanges="orientation|screenSize"
            android:label="Ongoing Quiz" />
        <activity
            android:name="com.test.quizexampleinterview.SplashScreenActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@style/FullscreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.test.quizexampleinterview.QuizInstructionActivity"
            android:label="Instruction" />
        <activity
            android:name="com.test.quizexampleinterview.QuizDeskBoardActivity"
            android:label="Desk Board" />
        <activity
            android:name="com.test.quizexampleinterview.QuizResultActivity"
            android:label="Quiz Result" />
        <activity android:name="com.test.quizexampleinterview.LoginActivity" />
    </application>

</manifest>